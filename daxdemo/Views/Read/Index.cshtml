@model daxdemo.Models.ReadViewModel

@{
    ViewData["Title"] = "Read";
    var elapsedTime = ViewData["TIME"].ToString();
}
<div class="row">
    <div class="col-6">
        <h4>Widgets</h4>
        <small>Execution time: <span class="text-success">@elapsedTime</span></small>
    </div>
    <div class="col-4"></div>
    <div class="col-2">
        @Html.ActionLink("Back", "Index", "Home", null, new { @class = "btn btn-secondary" })
    </div>
</div>
<hr />
<table class="table table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Date</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var widget in Model.Widgets)
        {
            <tr>
                <td>@widget.pk</td>
                <td>@widget.sk</td>
                <td>@widget.data</td>
            </tr>
        }
    </tbody>
</table>